    <converter:StringToShortStringConverter x:Key="StringToShortStringConverter" />
    <converter:InverseBooleanToVisibilityConverter x:Key="InverseBooleanToVisibilityConverter" />
    <converter:NullToVisibilityConveter x:Key="NullToVisibilityConveter" IsInverted="False"/>
    <converter:NullToVisibilityConveter x:Key="NotNullToVisibilityConveter" IsInverted="True"/>
